      "args": [
        "-I",
        "${workspaceRoot}",
        "${file}"
      ]
    },
    {
      "name": "RSpec - all",
      "type": "Ruby",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "/Users/jacobdilorenzo/.rbenv/versions/3.0.1/bin/rspec",
      "args": [
        "--pattern",
        "${workspaceRoot}/spec/**/*_spec.rb"
      ]
